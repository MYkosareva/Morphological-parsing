      "source": [
        "processed_data = []\n",
        "tmp_dict = {}\n",
        "\n",
        "for sentence in tqdm(data):\n",
        "  tmp = ''\n",
        "  for w in preprocess(sentence).split():\n",
        "    if w in tmp_dict:\n",
        "      tmp = tmp + tmp_dict[w] + ' '\n",
        "\n",
        "    else:\n",
        "      lemma = lemmatize(w)\n",
        "      tmp = tmp + lemma + ' '\n",
        "      tmp_dict[w] = lemma\n",
        "  processed_data += [tmp[:-1]]"
      ],
