    {
      "cell_type": "code",
      "metadata": {
        "id": "JFeQOqqrtRyw"
      },
      "source": [
        "" Задача: Лемматизировать текст (привести к словарной форме) и приписать леммам частеречные теги. (https://stepik.org/lesson/37845/step/1?unit=18887)""
        
        "from bs4 import BeautifulSoup\n",
        "from tqdm import tqdm\n",
        "import requests\n",
        "import re"
      ],
      "execution_count": 1,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "Vo1Lz2owHoYl"
      },
      "source": [
        "def read_txt(path):\n",
        "  with open(path, encoding=\"utf-8\") as file:\n",
        "    return [l.rstrip(\"\\n\") for l in file]"
      ],
      "execution_count": 2,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "UvruJw-iKX7c"
      },
      "source": [
        "def write_txt(path, data_):\n",
        "  with open(path, 'w') as output:\n",
        "    for row in data_:\n",
        "      output.write(row + '\\n')"
      ],
      "execution_count": 3,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "1hmJfS9WwZXa"
      },
      "source": [
        "def morph(word):\n",
        "  link = 'https://morfologija.ru/словоформа/'   # ссылка на online словарь\n",
        "  request = requests.get(link+word)\n",
        "  soup = BeautifulSoup(request.text, 'lxml')\n",
        "  try:\n",
        "    lem = soup.td.get_text()\n",
        "    pos = soup.find('b').next_sibling\n",
        "    return lem,pos\n",
        "  except:\n",
        "    return word, ' существительное'      # тк по статистически сущ чаще встречаются (для слов отсутствующих в словаре)"
      ],
      "execution_count": 5,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "uXS9seaREJML"
      },
      "source": [
        "def lemmatize(word_):\n",
        "  pos_tag = {' существительное': 'S',\n",
        "           ' инфинитив - ': 'V', \n",
        "           ' прилагательное': 'A', \n",
        "           ' кр. прилагательное': 'A',\n",
        "           ' предлог - ': 'PR', \n",
        "           ' союз - ': 'CONJ', \n",
        "           ' наречие - ': 'ADV',\n",
        "           ' междометие - ': 'ADV',\n",
        "           ' вводное слово - ': 'ADV',\n",
        "           ' частица - ': 'ADV',\n",
        "           ' местоимение': 'NI',\n",
        "           ' местоим. прил.': 'NI',\n",
        "           ' числительное': 'NI',\n",
        "           ' поряд. числительное': 'NI',\n",
        "           ' предикатив - ': 'NI',\n",
        "           ' ФРАЗ - ': 'NI'}\n",
        "  w1 = word_\n",
        "  w2 = morph(word_)[0]\n",
        "  w3 = pos_tag[morph(word_)[1]]\n",
        "  return '{}{{{}={}}}'.format(w1,w2,w3)\n"
      ],
      "execution_count": 6,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "7ySbI32KEJOO"
      },
      "source": [
        "def preprocess(string):\n",
        "  string = re.sub(r'[^\\w\\s]','',string,flags=re.UNICODE)\n",
        "  string = re.sub(r'[\\d]','',string,flags=re.UNICODE) # digits\n",
        "  return string"
      ],
      "execution_count": 7,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "1uv3Ut_7Hoa6"
      },
      "source": [
        "data = read_txt('/content/dataset_37845_1.txt')"
      ],
      "execution_count": 8,
      "outputs": []
    },
